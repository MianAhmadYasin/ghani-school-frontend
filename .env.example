# Frontend environment variables example
# Set your backend API base URL (must include /api/v1 if backend exposes it that way)
NEXT_PUBLIC_API_URL=https://your-backend.example.com/api/v1

# Disable Next.js telemetry
NEXT_TELEMETRY_DISABLED=1

# Enable standalone build output for Docker deployment (set to 'true' when building Docker)
NEXT_STANDALONE=true

# Optional: set Node environment
NODE_ENV=production
